@article{OKAZAKI201820,
title = {Meter-scale multi-loop capillary heat pipe},
journal = {Applied Thermal Engineering},
volume = {141},
pages = {20-28},
year = {2018},
issn = {1359-4311},
doi = {https://doi.org/10.1016/j.applthermaleng.2018.05.116},

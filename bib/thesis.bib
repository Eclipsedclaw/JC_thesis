@article{YAMAMOTO2013227,
title = {Search for cosmic-ray antiproton origins and for cosmological antimatter with BESS},
journal = {Advances in Space Research},
volume = {51},
number = {2},
pages = {227-233},
year = {2013},
note = {The Origins of Cosmic Rays: Resolving Hess's Century-Old Puzzle},
issn = {0273-1177},
doi = {https://doi.org/10.1016/j.asr.2011.07.012},
url = {https://www.sciencedirect.com/science/article/pii/S027311771100545X},
author = {A. Yamamoto and J.W. Mitchell and K. Yoshimura and K. Abe and H. Fuke and S. Haino and T. Hams and M. Hasegawa and A. Horikoshi and A. Itazaki and K.C. Kim and T. Kumazawa and A. Kusumoto and M.H. Lee and Y. Makida and S. Matsuda and Y. Matsukawa and K. Matsumoto and A.A. Moiseev and Z. Myers and J. Nishimura and M. Nozaki and R. Orito and J.F. Ormes and K. Sakai and M. Sasaki and E.S. Seo and Y. Shikaze and R. Shinoda and R.E. Streitmatter and J. Suzuki and Y. Takasugi and K. Takeuchi and K. Tanaka and T. Taniguchi and N. Thakur and T. Yamagami and T. Yoshida},
keywords = {Cosmic rays, Antiproton, Antimatter, Primordial black hole, Solar modulation, Solar BESS},
abstract = {The balloon-borne experiment with a superconducting spectrometer (BESS) has performed cosmic-ray observations as a US–Japan cooperative space science program, and has provided fundamental data on cosmic rays to study elementary particle phenomena in the early Universe. The BESS experiment has measured the energy spectra of cosmic-ray antiprotons to investigate signatures of possible exotic origins such as dark matter candidates or primordial black holes, and searched for heavier antinuclei that might reach Earth from antimatter domains formed in the early Universe. The apex of the BESS program was reached with the Antarctic flight of BESS-Polar II, during the 2007–2008 Austral Summer, that obtained over 4.7billion cosmic-ray events from 24.5days of observation. The flight took place at the expected solar minimum, when the sensitivity of the low-energy antiproton measurements to a primary source is greatest. Here, we report the scientific results, focusing on the long-duration flights of BESS-Polar I (2004) and BESS-Polar II (2007–2008).}
}


@article{RE2023167617,
title = {A mixed-signal processor for X-ray spectrometry and tracking in the GAPS experiment},
journal = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
volume = {1045},
pages = {167617},
year = {2023},
issn = {0168-9002},
